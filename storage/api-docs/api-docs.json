    "components": {
        "schemas": {
            "Order": {
                "title": "Order",
                "description": "Order model",
                "required": [
                    "people_qty",
                    "order_date",
                    "flight_num",
                    "status",
                    "phone",
                    "email",
                    "total",
                    "arrival_date"
                ],
                "properties": {
                    "id": {
                        "description": "ID of the order",
                        "type": "integer",
                        "example": 1
                    },
                    "people_qty": {
                        "description": "Number of people in the order",
                        "type": "integer",
                        "example": 3
                    },
                    "order_date": {
                        "description": "Order date and time",
                        "type": "string",
                        "format": "date-time",
                        "example": "2024-10-10T10:30:00Z"
                    },
                    "flight_num": {
                        "description": "Flight number associated with the order",
                        "type": "string",
                        "example": "AB1234"
                    },
                    "status": {
                        "description": "Order status",
                        "type": "string",
                        "example": "completed"
                    },
                    "phone": {
                        "description": "Phone number of the person who placed the order",
                        "type": "string",
                        "example": "+1234567890"
                    },
                    "email": {
                        "description": "Email address of the person who placed the order",
                        "type": "string",
                        "format": "email",
                        "example": "email@example.com"
                    },
                    "luggage_qty": {
                        "description": "Quantity of luggage for the order",
                        "type": "integer",
                        "example": 2
                    },
                    "total": {
                        "description": "Total price of the order",
                        "type": "number",
                        "format": "float",
                        "example": 120.5
                    },
                    "arrival_date": {
                        "description": "Arrival date and time",
                        "type": "string",
                        "format": "date-time",
                        "example": "2024-10-11T12:00:00Z"
                    },
                    "comment": {
                        "description": "Additional comments for the order",
                        "type": "string",
                        "example": "Please arrange for a VIP transfer."
                    }
                },
                "type": "object"
            },
            "Transfer": {
                "title": "Transfer",
                "description": "Transfer model",
                "required": [
                    "driver_id",
                    "people_qty",
                    "status",
                    "pickup",
                    "unload",
                    "destination"
                ],
                "properties": {
                    "id": {
                        "description": "ID of the transfer",
                        "type": "integer",
                        "example": 1
                    },
                    "driver_id": {
                        "description": "ID of the driver associated with the transfer",
                        "type": "integer",
                        "example": 123
                    },
                    "luggage": {
                        "description": "Details about the luggage",
                        "type": "string",
                        "example": "2 suitcases"
                    },
                    "people_qty": {
                        "description": "Number of people in the transfer",
                        "type": "integer",
                        "example": 3
                    },
                    "flight_num": {
                        "description": "Flight number for the transfer",
                        "type": "string",
                        "example": "AB1234"
                    },
                    "pickup": {
                        "description": "Pickup location",
                        "type": "string",
                        "example": "Airport Terminal 1"
                    },
                    "unload": {
                        "description": "Unload location",
                        "type": "string",
                        "example": "Hotel XYZ"
                    },
                    "gate": {
                        "description": "Gate number at the pickup location",
                        "type": "string",
                        "example": "Gate A"
                    },
                    "destination": {
                        "description": "Transfer destination",
                        "type": "string",
                        "example": "City Center"
                    },
                    "status": {
                        "description": "Current status of the transfer",
                        "type": "string",
                        "example": "completed"
                    },
                    "comment": {
                        "description": "Additional comments for the transfer",
                        "type": "string",
                        "example": "VIP transfer"
                    }
                },
                "type": "object"
            }
        }
